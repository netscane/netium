{
    "log": {
        "level": "info",
        "access": null,
        "error": null
    },
    "api": {
        "listen": "0.0.0.0:9090"
    },
    "inbounds": [
        {
            "tag": "vmess-in",
            "protocol": "vmess",
            "listen": "0.0.0.0:443",
            "settings": {
                "users": [
                    {
                        "uuid": "your-uuid-here",
                        "alter_id": 0,
                        "email": "user@example.com"
                    }
                ]
            },
            "transport": {
                "address": "0.0.0.0",
                "port": 443,
                "transport_type": "websocket",
                "tcp_settings": null,
                "ws_settings": {
                    "path": "/ws",
                    "headers": {}
                },
                "tls_settings": {
                    "enabled": true,
                    "server_name": "your-server.com",
                    "allow_insecure": false,
                    "certificate_file": "/path/to/server.crt",
                    "key_file": "/path/to/server.key"
                }
            }
        }
    ],
    "outbounds": [
        {
            "tag": "direct",
            "protocol": "direct",
            "settings": null,
            "transport": null
        }
    ],
    "routing": {
        "domain_strategy": "AsIs",
        "rules": []
    }
}
